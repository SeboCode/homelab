{
  "description": "Allow access to nextcloud service",

  "service": {
    "nextcloud": [
      { "proto": "tcp", "port": 20100 },
      { "proto": "udp", "port": 20100 }
    ]
  },

  "filter": [
    {
      "in": "internet",
      "out": "_fw",
      "service": "nextcloud",
      "action": "accept"
    }
  ]
}
